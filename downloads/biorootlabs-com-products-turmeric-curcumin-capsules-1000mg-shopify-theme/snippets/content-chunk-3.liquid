                      .then((res) => res.json())
                      .then(() => {
                        // Update cart sections
                        return fetch("/?sections=cart-drawer,cart-icon-bubble")
                          .then((res) => res.json())
                          .then((sections) => {
                            // Update cart bubble
                            const bubble = document.querySelector("#cart-icon-bubble");
                            if (bubble && sections["cart-icon-bubble"]) {
                              bubble.innerHTML = sections["cart-icon-bubble"];
                            }

                            // Update and open cart drawer
                            const drawer = document.querySelector("cart-drawer");
                            if (drawer && sections["cart-drawer"]) {
                              const temp = document.createElement("div");
                              temp.innerHTML = sections["cart-drawer"];
                              const newDrawer = temp.querySelector("cart-drawer");
                              if (newDrawer) {
                                drawer.replaceWith(newDrawer);
                                // Open drawer after replacing
                                if (newDrawer.open) {
                                  newDrawer.open();
                                }
                              }
                            } else if (!drawer) {
                              // Fallback if no drawer exists
                              window.location.href = "/cart";
                            }
                          });
                      })
                      .catch((err) => {
                        console.error("[Upsell Add to Cart Error]", err);
