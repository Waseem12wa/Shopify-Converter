                  if (!upsellBtn) return;

                  const variantId = upsellBtn.dataset.upsellVariant;
                  const isAvailable = upsellBtn.dataset.upsellAvailable === "true";
                  
                  if (!isAvailable) {
                    upsellBtn.disabled = true;
                    upsellBtn.querySelector('span').textContent = "Sold Out";
                    return;
                  }

                  if (!variantId) return;

                  upsellBtn.addEventListener("click", (e) => {
                    e.preventDefault();

                    // Add loading state
                    upsellBtn.classList.add("is-loading");
                    upsellBtn.disabled = true;

                    fetch("/cart/add.js", {
                      method: "POST",
                      headers: { "Content-Type": "application/json" },
                      body: JSON.stringify({
                        items: [{ id: variantId, quantity: 1 }],
                      }),
