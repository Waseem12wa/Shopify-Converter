  <script>
    document.addEventListener("DOMContentLoaded", function() {
      function getNextWeekday(date) {
        var day = date.getDay();
        var daysToAdd = (day === 5) ? 3 : (day === 6) ? 2 : 1;
        return new Date(date.setDate(date.getDate() + daysToAdd));
      }

      var today = new Date();
      var shipDate = getNextWeekday(today);
      
      // Format date to have short month (e.g., "Thu, Feb 22")
      var options = { weekday: 'short', month: 'short', day: 'numeric' };
      var formattedDate = shipDate.toLocaleDateString('en-US', options);
      document.getElementById('shipping-date').textContent = formattedDate;

      // Fetch country data using GeoJS API
      fetch('https://get.geojs.io/v1/ip/geo.json')
        .then(response => response.json())
        .then(data => {
          console.log("Geolocation API Response:", data); // Debugging log

          if (!data || !data.country_code) {
            throw new Error("Invalid data from API");
          }
          
          var countryCode = data.country_code.toUpperCase(); // Get country code (e.g., "GB", "US")
          var flagUrl = `https://flagcdn.com/w40/${countryCode.toLowerCase()}.png`;

          // Set flag and country code
          document.getElementById('country-flag').src = flagUrl;
          document.getElementById('country-flag').alt = countryCode + " Flag";
          document.getElementById('country-flag').style.display = "inline";
          document.getElementById('country-text').innerHTML = `Fast <span class="bold">${countryCode}</span> Shipping`;
        })
        .catch(error => {
          console.error('Error fetching the IP information:', error);
          document.getElementById('country-flag').style.display = "none"; 
          document.getElementById('country-text').innerHTML = "Fast Worldwide Shipping";
        });
    });
