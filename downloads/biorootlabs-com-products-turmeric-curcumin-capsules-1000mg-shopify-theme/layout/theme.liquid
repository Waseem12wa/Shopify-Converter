<script>
(function () {
  const LOG = "[Loop â Shopify Price Sync]";
  let isSyncing = false;
  let lastAppliedPrice = null;

  console.log(LOG, "Script loaded");

  function getLoopSelectedPrice() {
    const el = document.querySelector(
      '.loop-widget-purchase-option-selected .loop-widget-purchase-option-price'
    );
    return el ? el.textContent.trim() : null;
  }

  function syncShopifyPrice() {
    if (isSyncing) return;

    const loopPrice = getLoopSelectedPrice();
    if (!loopPrice) return;

    // ð Guard: already applied
    if (loopPrice === lastAppliedPrice) return;

    isSyncing = true;
    lastAppliedPrice = loopPrice;

    console.log(LOG, "Syncing price â", loopPrice);

    document
      .querySelectorAll('.price-item--sale.price-item--last')
      .forEach(el => {
        if (el.textContent.trim() !== loopPrice) {
          el.textContent = loopPrice;
        }
      });

    document
      .querySelectorAll('.price-item--regular.main-price')
      .forEach(el => {
        if (el.textContent.trim() !== loopPrice) {
          el.textContent = loopPrice;
        }
      });

    // ð§ Release lock next frame
    requestAnimationFrame(() => {
      isSyncing = false;
    });
  }

  /* ð§  BOOTSTRAP: wait until DOM exists */
  const bootObserver = new MutationObserver(() => {
    const loopContainer = document.querySelector('.loop-widget-container');
    const priceBlock = document.querySelector('.price');

    if (!loopContainer || !priceBlock) return;

    console.log(LOG, "DOM ready â observers attached");

    const loopObserver = new MutationObserver(syncShopifyPrice);
    loopObserver.observe(loopContainer, {
      childList: true,
      subtree: true
    });

    const priceObserver = new MutationObserver(syncShopifyPrice);
    priceObserver.observe(priceBlock, {
      childList: true,
      subtree: true
    });

    syncShopifyPrice();
    bootObserver.disconnect();
  });

  bootObserver.observe(document.documentElement, {
    childList: true,
    subtree: true
  });
})();
</script><script>
