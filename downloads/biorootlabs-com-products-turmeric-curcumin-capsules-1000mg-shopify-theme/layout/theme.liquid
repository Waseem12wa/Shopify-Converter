document.addEventListener("kaching-bundles-block-loaded", (event) => {
  const productForm = document.querySelector('form[data-type="add-to-cart-form"]');
  const kachingBundlesBlock = event.detail.component;

  if (!kachingBundlesBlock || !productForm) return;

  const bundleMap = {
    'Buy 2 Get 1 Free': '690222268756',
    'Buy 3 Get 2 Free': '690172625236',
    'Buy 1 Bottle': '690172559700'
  };

  const updateSellingPlan = (bundleTitle) => {
    const newSellingPlanId = bundleMap[bundleTitle];
    if (!newSellingPlanId) {
      console.log(`Bundle selected: ${bundleTitle} â No matching selling_plan ID found`);
      return;
    }

    const sellingPlanInput = productForm.querySelector('input[name="selling_plan"]');
    const sellingPlanSelect = document.querySelector(
      '.kaching-bundles__bar-variant-select.kaching-bundles__selling-plan-select'
    );

    if (sellingPlanInput && sellingPlanInput.value !== newSellingPlanId) {
      sellingPlanInput.value = newSellingPlanId;
      sellingPlanInput.dispatchEvent(new Event('input', { bubbles: true }));
      sellingPlanInput.dispatchEvent(new Event('change', { bubbles: true }));
      console.log(`Updated selling_plan input to ${newSellingPlanId}`);
    }

    if (sellingPlanSelect && sellingPlanSelect.value !== newSellingPlanId) {
      sellingPlanSelect.value = newSellingPlanId;
      sellingPlanSelect.dispatchEvent(new Event('input', { bubbles: true }));
      sellingPlanSelect.dispatchEvent(new Event('change', { bubbles: true }));
      console.log(`Updated selling_plan select to ${newSellingPlanId}`);
    }
  };

  kachingBundlesBlock.addEventListener('deal-bar-selected', () => {
    const currentDeal = kachingBundlesBlock.querySelector('.kaching-bundles__bar--selected');
    if (!currentDeal) {
      console.log('No selected deal found');
      return;
    }

    const dealTitleEl = currentDeal.querySelector('.kaching-bundles__bar-title');
    if (!dealTitleEl) {
      console.log('Selected deal has no title element');
